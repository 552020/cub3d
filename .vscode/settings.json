{
  "[c]": {
    "editor.defaultFormatter": "keyhr.42-c-format",
    "editor.formatOnSave": true
    // "editor.formatOnSave": false
  },
  "editor.insertSpaces": false,
  "editor.tabSize": 4,
  "cSpell.words": ["isregularchar", "isspecialchar", "isvalidvarname", "putendl"],
  "workbench.colorCustomizations": {
    "activityBar.background": "#203210",
    "titleBar.activeBackground": "#2D4717",
    "titleBar.activeForeground": "#F9FCF6"
  },
  "files.associations": {
    ".clang-format-google": "c",
    "complex": "c",
    "__bit_reference": "c",
    "__hash_table": "c",
    "__split_buffer": "c",
    "array": "c",
    "initializer_list": "c",
    "ios": "c",
    "string": "c",
    "string_view": "c",
    "unordered_map": "c",
    "vector": "c",
    "__locale": "c",
    "__string": "c",
    "cub3d.h": "c",
    "stat.h": "c",
    "*.tcc": "cpp",
    "ostream": "cpp"
  }
}
